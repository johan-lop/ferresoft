	<queryString>
		<![CDATA[select v.subtotal, v.fecha_factura, v.fecha_vencimiento, v.subtotal, v.total_iva, v.total,
c.nombre, c.numero_documento, c.direccion, c.telefono, c.celular, m.descripcion as ciudad,
c.contacto, v.numero_factura
from transaccional.venta v 
inner join maestro.cliente c
